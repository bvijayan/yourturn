<h2>{{app}} Versions</h2>

<div class="btn-group">
    <a href="application/detail/{{app}}" class="btn btn-default">
        <i class="fa fa-chevron-left"></i> Back to {{app}}
    </a>
</div>

{{#if versions.length}}
    <ul>
        {{#each versions}}
            <li>
                {{this.id}}
            </li>
        {{/each}}
    </ul>
{{else}}
    No versions to display.
{{/if}}
